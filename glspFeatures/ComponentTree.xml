							<feature name="C1221 eclipse-glsp/client"/>
							<feature name="C1222 Model.ts"/>
						</and>
						<and abstract="true" mandatory="true" name="C123 Mapping to ShapeModel Views">
							<feature name="C1231 eclipse-glsp/client/views"/>
							<feature name="C1232 views.tsx"/>
						</and>
					</and>
				</and>
				<alt abstract="true" name="C2 Client View Port">
					<feature abstract="true" name="Center"/>
					<feature abstract="true" name="Fit to Screen"/>
				</alt>
				<feature abstract="true" name="C3 Client Status Notification"/>
				<feature abstract="true" name="C4 Element Editing"/>
				<feature abstract="true" name="C5 Element Validation"/>
				<feature abstract="true" name="C6 Model Layout"/>
				<feature abstract="true" name="C7 Contexts"/>
