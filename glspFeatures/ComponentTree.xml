				</and>
				<feature abstract="true" name="Node Change Container"/>
				<feature abstract="true" name="Edge Reconnect"/>
				<feature abstract="true" name="Edge Routing Points"/>
				<feature abstract="true" name="Ghost Elements ???"/>
				<feature abstract="true" name="Element Text Editing"/>
				<feature abstract="true" name="Clipboard"/>
				<feature abstract="true" name="Undo / Redo"/>
			</and>
		</and>
	</struct>
</featureModel>
