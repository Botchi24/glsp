	<struct>
		<and abstract="true" mandatory="true" name="glspFeatures">
			<and abstract="true" mandatory="true" name="S">
				<and abstract="true" mandatory="true" name="S1 Metamodel Extension">
					<graphics key="collapsed" value="false"/>
					<alt abstract="true" mandatory="true" name="S11 SourceModel">
						<alt abstract="true" name="S111 Types">
							<feature abstract="true" name="S1111 Java Classes / EMF"/>
							<feature abstract="true" name="S1112 GModel Extension / EMF"/>
							<feature abstract="true" name="S1113 other SourceModel"/>
						</alt>
						<feature name="S112 ModelPackage + ModelFactory"/>
						<alt abstract="true" name="S113 Integration">
							<feature name="S11121 override glsp.server.emf.notation.EMFNotationSourceModelStorage.setupResourceSet">
								<description>load ModelPackage</description>
							</feature>
							<feature name="S11122 implement glsp.graph.GraphExtension">
								<description>load ModelPackage + ModelFactory</description>
							</feature>
						</alt>
					</alt>
					<and abstract="true" mandatory="true" name="S12 ModelTypes">
						<graphics key="collapsed" value="false"/>
						<feature mandatory="true" name="S121 use glsp.graph.DefaultTypes"/>
						<and abstract="true" mandatory="true" name="S122 extended Types">
							<feature mandatory="true" name="S1221 utils.ModelTypes"/>
							<feature mandatory="true" name="S1222 override glsp.server.diagram.BaseDiagramConfiguration.getTypeMappings">
								<description>map to glsp.graph.GraphPackage.literals</description>
							</feature>
						</and>
					</and>
					<alt abstract="true" mandatory="true" name="S13 GModel Creation">
						<feature name="S131 override EMFNotationGModelFactory.fillRootElement">
							<description>add Shape Model to GNodes</description>
						</feature>
						<feature abstract="true" name="S132 SourceModel extends GModel"/>
					</alt>
				</and>
				<alt abstract="true" mandatory="true" name="S2 load Input File">
					<graphics key="collapsed" value="false"/>
					<feature name="S21 glsp.server.emf.notation.EMFNotationSourceModelStorage">
						<description>
							load elements with EMFNotationSourceModelStorage.loadSemanticModel
							load position and size with EMFNotationSourceModelStorage.loadNotationModel
						</description>
					</feature>
					<feature name="S22 glsp.server.gmodel.GModelStorage.loadSourceModel"/>
				</alt>
				<alt abstract="true" mandatory="true" name="S3 Element Creation">
					<graphics key="collapsed" value="false"/>
					<feature name="S31 override glsp.server.operations.OperationHandler.createCommand"/>
					<feature name="S32 implement utils.Builder"/>
				</alt>
				<and abstract="true" name="S4 Element Deletion">
					<graphics key="collapsed" value="false"/>
					<feature abstract="true" mandatory="true" name="remove element"/>
					<feature abstract="true" mandatory="true" name="remove shape"/>
				</and>
				<feature abstract="true" name="S5 Element Editing"/>
				<feature abstract="true" name="S6 Element Validation"/>
				<feature abstract="true" name="S7 Model Layout"/>
				<and abstract="true" name="S8 Contexts">
					<graphics key="collapsed" value="false"/>
					<feature abstract="true" name="Context Menu"/>
					<feature abstract="true" name="Command Palette"/>
					<feature abstract="true" name="Tool Palette"/>
				</and>
			</and>
			<and abstract="true" mandatory="true" name="C">
				<graphics key="collapsed" value="false"/>
				<and abstract="true" name="C1 Metamodel Extension">
