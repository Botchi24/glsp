<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<featureModel>
	<properties>
		<graphics key="autolayoutconstraints" value="false"/>
		<graphics key="legendautolayout" value="true"/>
		<graphics key="showconstraints" value="true"/>
		<graphics key="showshortnames" value="false"/>
		<graphics key="layout" value="vertical"/>
		<graphics key="showcollapsedconstraints" value="true"/>
		<graphics key="legendhidden" value="false"/>
		<graphics key="layoutalgorithm" value="4"/>
	</properties>
	<struct>
		<and abstract="true" mandatory="true" name="glspFeatures">
			<and abstract="true" mandatory="true" name="Server">
				<and abstract="true" mandatory="true" name="SM">
					<alt abstract="true" mandatory="true" name="SourceModelStorage">
						<feature name="EMFSourceModelStorage"/>
						<feature name="GModelStorage"/>
					</alt>
					<alt abstract="true" mandatory="true" name="load SM on M1">
						<graphics key="collapsed" value="false"/>
						<feature name="load ModelPackage into EMF resourceSet"/>
						<and name="implement glsp.graph.GraphExtension">
							<feature mandatory="true" name="load ModelPackage"/>
							<feature mandatory="true" name="load ModelFactory"/>
						</and>
					</alt>
				</and>
				<and abstract="true" mandatory="true" name="SM2GM on M1">
					<graphics key="collapsed" value="false"/>
					<alt abstract="true" mandatory="true" name="GM extension">
						<feature abstract="true" name="not extending GM"/>
						<feature abstract="true" name="extending GM">
							<graphics key="collapsed" value="false"/>
						</feature>
					</alt>
					<alt abstract="true" mandatory="true" name="map ModelTypes">
						<feature name="to glsp.graph.DefaultTypes"/>
						<or name="to literals">
							<feature name="to glsp.graph.GraphPackage.Literals"/>
							<feature name="to custom GraphPackage.Literals"/>
						</or>
					</alt>
				</and>
			</and>
			<feature abstract="true" mandatory="true" name="Client"/>
		</and>
	</struct>
	<constraints>
		<rule>
			<imp>
				<var>not extending GM</var>
				<var>EMFSourceModelStorage</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>EMFSourceModelStorage</var>
				<var>load ModelPackage into EMF resourceSet</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>extending GM</var>
				<var>GModelStorage</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>GModelStorage</var>
				<var>implement glsp.graph.GraphExtension</var>
			</imp>
		</rule>
	</constraints>
</featureModel>
